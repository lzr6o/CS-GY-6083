<h2>Edit Room</h2>
<form action="/rooms/<%= room.id %>?_method=PUT" method="POST">
    <%- include('_form_fields') %>
    <div class="form-row form-row-end btn-row">
        <a class="btn btn-primary" href="/rooms">Cancel</a>
        <button class="btn btn-danger"
        type="submit">Update</button>
    </div>
</form>