<h2>Search Room</h2>
<form action="/rooms" method="GET">
    <div class="form-row">
        <div class="form-item">
            <label>Number</label>
            <input type="number" name="number" value="<%= searchOptions.number %>">
        </div>
    </div>
    <div class="form-row form-row-end btn-row">
        <button class = "btn btn-primary"
        type="submit">Search</button>
    </div>
</form>
<br>
<div>
    <% rooms.forEach(room => { %>
        <a href="/rooms/<%= room.id %>">
            <img height="100" width="150" src="<%= room.coverImagePath %>">
        </a>
        <% }) %>
</div>