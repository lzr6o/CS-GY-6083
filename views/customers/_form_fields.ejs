<div>
    <label>Username</label>
    <input type="text" name="username" value="<%= customer.username %>">
</div>
<div>
    <label>Password</label>
    <input type="password" name="password" value="<%= customer.password %>">
</div>
<div>
    <label>First Name</label>
    <input type="text" name="firstName" value="<%= customer.firstName %>">
</div>
<div>
    <label>Last Name</label>
    <input type="text" name="lastName" value="<%= customer.lastName %>">
</div>
<div>
    <label>Gender</label>
    <select name="gender">
        <option value="Male">Male</option>
        <option value="Female">Female</option>
    </select>
</div>
<div>
    <label>Email</label>
    <input type="email" name="email" value="<%= customer.email %>">
</div>
<div>
    <label>Phone</label>
    <input type="number" name="phone" value="<%= customer.phone %>">
</div>
<div>
    <label>Street</label>
    <input type="text" name="street" value="<%= customer.street %>">
</div>
<div>
    <label>City</label>
    <input type="text" name="city" value="<%= customer.city %>">
</div>
<div>
    <label>Zipcode</label>
    <input type="number" name="zipcode" value="<%= customer.zipcode %>">
</div>
<div>
    <label>State</label>
    <input type="text" name="state" value="<%= customer.state %>">
</div>
<div>
    <label>Country</label>
    <input type="text" name="country" value="<%= customer.country %>">
</div>
<div>
    <label>Identification Type</label>
    <select name="identificationType">
        <option value="Passport">Passport</option>
        <option value="SSN">SSN</option>
        <option value="Driver License">Driver License</option>
    </select>
</div>
<div>
    <label>Identification Number</label>
    <input type="text" name="identificationNumber" value="<%= customer.identificationNumber %>">
</div>
<div>
    <label>Cover</label>
    <input type="file" name="cover" class="filepond">
</div>