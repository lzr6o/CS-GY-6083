<h1>Invoice</h1>
<div>
    ID: <%= invoice.id %>
</div>
<div>
    Date Generated: <%= invoice.dateGenerated.toDateString() %>
</div>
<div>
    Amount: <%= invoice.amount %>
</div>

<div class="form-row form-row-end btn-row">
    <a class="btn btn-primary" href="/rentals/<%= invoice.rental.id %>">View Rental</a>
</div>

<div class="form-row form-row-end btn-row">
    <a class="btn btn-primary" href="/customers/<%= invoice.customer.id %>">View Customer</a>
</div>