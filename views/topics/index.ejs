<h2 class="page-header">Search Topic</h2>
<form action="/topics" method="GET">
    <div class="form-row">
        <div class="form-item">
            <label>Type</label>
            <input type="text" name="type" value="<%= searchOptions.type %>">
        </div>
    </div>
    <div class="form-row form-row-end btn-row">
        <button class = "btn btn-primary"
        type="submit">Search</button>
    </div>
</form>
<br>
<%- include('../partials/topicGrid', { topics: topics, large:true }) %>